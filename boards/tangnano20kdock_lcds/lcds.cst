// System Clock (27MHz)
IO_LOC  "sys_clk" 4;  // 27MHZ Crystal Oscillator


//IO_LOC  "sys_clk" 11;  // SY5351 clock 1
IO_PORT "sys_clk" IO_TYPE=LVCMOS33;


// Button 1
IO_LOC  "btn1" 88;
IO_PORT "btn1" PULL_MODE=DOWN IO_TYPE=LVCMOS33;

// UART
IO_LOC  "ser_tx" 69;
IO_PORT "ser_tx" IO_TYPE=LVCMOS33;
IO_LOC  "ser_rx" 70;
IO_PORT "ser_rx" PULL_MODE=UP IO_TYPE=LVCMOS33;

// Keyboard
IO_LOC  "ps2_clk" 73;
IO_PORT "ps2_clk" IO_TYPE=LVCMOS33 PULL_MODE=UP;
IO_LOC  "ps2_data" 74;
IO_PORT "ps2_data" IO_TYPE=LVCMOS33 PULL_MODE=UP;

// Data bus
IO_LOC "data[7]" 77;
IO_PORT "data[7]" IO_TYPE=LVCMOS33 DRIVE=12;
IO_LOC "data[6]" 27;
IO_PORT "data[6]" IO_TYPE=LVCMOS33 DRIVE=12;
IO_LOC "data[5]" 28;
