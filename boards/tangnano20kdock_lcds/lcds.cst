IO_LOC "data[1]" 30;
IO_PORT "data[1]" IO_TYPE=LVCMOS33 DRIVE=12;
IO_LOC "data[0]" 31;
IO_PORT "data[0]" IO_TYPE=LVCMOS33 DRIVE=12;

// Joystick
IO_LOC "js_clk" 71;
IO_PORT "js_clk" IO_TYPE=LVCMOS33;
IO_LOC "js_load_n" 72;
IO_PORT "js_load_n" IO_TYPE=LVCMOS33;
IO_LOC "js_data" 76;
IO_PORT "js_data" IO_TYPE=LVCMOS33 PULL_MODE=UP;

// LEDs
